<div style="width: 35%; height: 35%;" class="container pt-5">
  <!-- Default form login -->
  <form
    class="text-center border border-light p-5"
    action="/users/final-password-change/?token=<%= token %>  "
    method="POST"
  >
    <p class="h4 mb-4">New Password</p>

    <!-- Email -->
    <input
      type="password"
      id="defaultLoginFormEmail"
      class="form-control mb-4"
      placeholder="password"
      name="password"
      required
    />

    <!-- Sign in button -->
    <button class="btn btn-info btn-block my-4" type="submit">
      Change
    </button>
  </form>
  <!-- Default form login -->
</div>
